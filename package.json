{
  "name": "shop-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon -r tsconfig-paths/register src/app.ts",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts && npm run lint:fix",
    "format:check": "prettier --check src/**/*.ts",
    "check": "npm run lint && npm run format:check",
    "swagger-autogen": "node ./docs/swagger.js"
  },
  "main": "dist/app.js",
  "dependencies": {
    "@types/bcrypt-nodejs": "^0.0.31",
    "@types/csurf": "^1.11.5",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/module-alias": "^2.0.4",
    "@types/multer": "^1.4.12",
    "@types/node-cron": "^3.0.11",
    "@types/stripe": "^8.0.416",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/uuid": "^10.0.0",
    "@vercel/blob": "^0.26.0",
    "bcrypt-nodejs": "^0.0.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.0",
    "http-errors": "~1.6.3",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.4",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "rimraf": "^6.0.1",
    "stripe": "^17.2.1",
    "swagger-autogen": "^2.23.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.4",
    "prettier": "^3.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2"
  }
